{
  "extends": [
    "tslint:latest"
  ],
  "rules": {
    "array-type": false,
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "max-line-length": [
      true,
      {
        "limit": 120,
        "ignore-pattern": "^import |^export {(.*?)}"
      }
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "object-literal-sort-keys": false,
    "one-line": false,
    "no-object-literal-type-assertion": false,
    "no-this-assignment": false,
    "semicolon": [
      true,
      "never"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "quotemark": [
      true,
      "single"
    ],
    "variable-name": [
      true,
      "allow-leading-underscore"
    ]
  }
}